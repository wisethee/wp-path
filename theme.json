{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "background": true,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "link": true,
      "text": true,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#036D37"
        },
        {
          "name": "On Primary",
          "slug": "on-primary",
          "color": "#FFFFFF"
        },
        {
          "name": "Primary Container",
          "slug": "primary-container",
          "color": "#9CF6B2"
        },
        {
          "name": "On Primary Container",
          "slug": "on-primary-container",
          "color": "#00210C"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#506352"
        },
        {
          "name": "On Secondary",
          "slug": "on-secondary",
          "color": "#FFFFFF"
        },
        {
          "name": "Secondary Container",
          "slug": "secondary-container",
          "color": "#D2E8D3"
        },
        {
          "name": "On Secondary Container",
          "slug": "on-secondary-container",
          "color": "#0E1F12"
        },
        {
          "name": "Surface",
          "slug": "surface",
          "color": "#FBFDF7"
        },
        {
          "name": "On Surface",
          "slug": "on-surface",
          "color": "#191C19"
        },
        {
          "name": "Surface Variant",
          "slug": "surface-variant",
          "color": "#DDE5DA"
        },
        {
          "name": "On Surface Variant",
          "slug": "on-surface-variant",
          "color": "#414941"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fluid": true,
      "fontStyle": false,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": true,
      "textDecoration": true,
      "textTransform": true,
      "fontFamilies": [
        {
          "fontFamily": "Jost, sans-serif",
          "name": "Sans",
          "slug": "sans",
          "fontFace": [
            {
              "fontFamily": "Jost",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "fontWeight": "400 700",
              "src": ["file:./assets/fonts/jost/jost-variable-font.ttf"]
            }
          ]
        },
        {
          "fontFamily": "'SF Mono', ui-monospace, Menlo, Consolas, Monaco, monospace",
          "name": "Monospace",
          "slug": "monospace"
        }
      ],
      "fontSizes": [
        {
          "name": "Body Small",
          "slug": "body-small",
          "size": "clamp(0.875rem, 0.7647rem + 0.2941vw, 1rem)"
        },
        {
          "name": "Body Medium",
          "slug": "body-medium",
          "size": "clamp(1rem, 0.8897rem + 0.2941vw, 1.125rem)"
        },
        {
          "name": "Body Large",
          "slug": "body-large",
          "size": "clamp(1.125rem, 1.0147rem + 0.2941vw, 1.25rem)"
        },
        {
          "name": "Label Small",
          "slug": "label-small",
          "size": "clamp(0.75rem, 0.6397rem + 0.2941vw, 0.875rem)"
        },
        {
          "name": "Label Medium",
          "slug": "label-medium",
          "size": "clamp(0.875rem, 0.7647rem + 0.2941vw, 1rem)"
        },
        {
          "name": "Label Large",
          "slug": "label-large",
          "size": "clamp(1rem, 0.8897rem + 0.2941vw, 1.125rem)"
        },
        {
          "name": "Title Small",
          "slug": "title-small",
          "size": "clamp(1.125rem, 1.0147rem + 0.2941vw, 1.25rem)"
        },
        {
          "name": "Title Medium",
          "slug": "title-medium",
          "size": "clamp(1.25rem, 1.0294rem + 0.5882vw, 1.5rem)"
        },
        {
          "name": "Title Large",
          "slug": "title-large",
          "size": "clamp(1.5rem, 1.0588rem + 1.1765vw, 2rem)"
        },
        {
          "name": "Headline Small",
          "slug": "headline-small",
          "size": "clamp(2rem, 1.7794rem + 0.5882vw, 2.25rem)"
        },
        {
          "name": "Headline Medium",
          "slug": "headline-medium",
          "size": "clamp(2.25rem, 2.0294rem + 0.5882vw, 2.5rem)"
        },
        {
          "name": "Headline Large",
          "slug": "headline-large",
          "size": "clamp(2.5rem, 2.0588rem + 1.1765vw, 3rem)"
        },
        {
          "name": "Display Small",
          "slug": "display-small",
          "size": "clamp(3rem, 2.3382rem + 1.7647vw, 3.75rem)"
        },
        {
          "name": "Display Medium",
          "slug": "display-medium",
          "size": "clamp(3.75rem, 3.0882rem + 1.7647vw, 4.5rem)"
        },
        {
          "name": "Display Large",
          "slug": "display-large",
          "size": "clamp(4.5rem, 3.1765rem + 3.5294vw, 6rem)"
        }
      ]
    },
    "layout": {
      "contentSize": "1308px",
      "wideSize": "1308px"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"],
      "blockGap": true
    },
    "border": {},
    "blocks": {},
    "custom": {
      "typography": {
        "line-height": {
          "body-small": "24px",
          "body-medium": "24px",
          "body-large": "28px",
          "label-small": "20px",
          "label-medium": "24px",
          "label-large": "24px",
          "title-small": "20px",
          "title-medium": "24px",
          "title-large": "32px",
          "headline-small": "36px",
          "headline-medium": "40px",
          "headline-large": "48px",
          "display-small": "60px",
          "display-medium": "72px",
          "display-large": "96px"
        }
      },
      "spacing": {
        "p-x": "clamp(1rem, -1.1429rem + 5.7143vw, 4rem)",
        "p-y": "clamp(0.5625rem, 0.0268rem + 1.4286vw, 1.3125rem)"
      }
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--on-surface-variant)",
      "background": "var(--wp--preset--color--surface)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--sans)",
      "fontSize": "var(--wp--preset--font-size--body-medium)",
      "lineHeight": "var(--wp--custom--typography--line-height--body-medium)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--headline-large)",
          "lineHeight": "var(--wp--custom--typography--line-height--headline-large)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--headline-medium)",
          "lineHeight": "var(--wp--custom--typography--line-height--headline-medium)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--headline-small)",
          "lineHeight": "var(--wp--custom--typography--line-height--headline-small)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--title-large)",
          "lineHeight": "var(--wp--custom--typography--line-height--title-large)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--title-medium)",
          "lineHeight": "var(--wp--custom--typography--line-height--title-medium)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--title-small)",
          "lineHeight": "var(--wp--custom--typography--line-height--title-small)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--label-medium)",
          "fontWeight": "600",
          "letterSpacing": "0.03em"
        },
        "spacing": {
          "padding": {
            "top": "6px",
            "right": "16px",
            "bottom": "6px",
            "left": "16px"
          }
        }
      },
      "core/navigation": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--label-medium)",
          "fontWeight": "600",
          "letterSpacing": "0.03em"
        }
      }
    }
  }
}
